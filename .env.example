# Example environment variables for MakerWorks
# Copy this file to .env and fill in the values

# Backend API base URL
VITE_API_BASE_URL=http://localhost:3000

# Authentik configuration
VITE_AUTHENTIK_BASE_URL=https://authentik.yourdomain.com
VITE_AUTH_CLIENT_ID=your-client-id
VITE_AUTH_CLIENT_SECRET=your-client-secret
VITE_AUTHENTIK_LOGIN_URL=https://authentik.yourdomain.com/application/o/authorize/
VITE_AUTHENTIK_REGISTER_URL=https://authentik.yourdomain.com/if/flow/registration/
VITE_AUTHENTIK_LOGOUT_URL=https://authentik.yourdomain.com/if/session-end/
